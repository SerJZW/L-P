<Application x:Class="L_P.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:L_P"
             StartupUri="View\Window\MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/RegStyles.xaml"/>
                <ResourceDictionary Source="/Styles/UserStyles.xaml"/>
                <ResourceDictionary Source="/Styles/MainStyles.xaml"/>
                <ResourceDictionary Source="/Styles/ListsStyle.xaml"/>
                <ResourceDictionary Source="/Styles/WindowStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
